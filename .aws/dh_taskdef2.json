{
    "containerDefinitions": [
        {
            "name": "strapi-app",
            "image": "docker.io/adamsia/test-strapi:latest",
            "portMappings": [
                {
                    "containerPort": 1337,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "entryPoint": [
                "sh",
                "-c"
            ],
            "command": ["yarn start"],
            "secrets": [
                {
                    "name": "APP_KEYS",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:806292820880:secret:poc/ldr/strapi/cicd/demo-rKpw7W:APP_KEYS::"
                },
                {
                    "name": "API_TOKEN_SALT",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:806292820880:secret:poc/ldr/strapi/cicd/demo-rKpw7W:API_TOKEN_SALT::"
                },
                {
                    "name": "ADMIN_JWT_SECRET",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:806292820880:secret:poc/ldr/strapi/cicd/demo-rKpw7W:ADMIN_JWT_SECRET::"
                },
                {
                    "name": "TRANSFER_TOKEN_SALT",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:806292820880:secret:poc/ldr/strapi/cicd/demo-rKpw7W:TRANSFER_TOKEN_SALT::"
                },
                {
                    "name": "DATABASE_CLIENT",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:806292820880:secret:poc/ldr/strapi/cicd/demo-rKpw7W:DATABASE_CLIENT::"
                },
                {
                    "name": "DATABASE_FILENAME",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:806292820880:secret:poc/ldr/strapi/cicd/demo-rKpw7W:DATABASE_FILENAME::"
                },
                {
                    "name": "JWT_SECRET",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:806292820880:secret:poc/ldr/strapi/cicd/demo-rKpw7W:JWT_SECRET::"
                }
            ]
        }
    ],
    "family": "FAM-FG-LDR-DH-TASK",
    "executionRoleArn": "arn:aws:iam::806292820880:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "1024"
}
