{
  "containerDefinitions": [
    {
      "name": "strapi-app",
      "image": "docker.io/adamsia/test-strapi:latest",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "entryPoint": ["sh", "-c"],
      "command": ["yarn start"]
    }
  ],
  "family": "FAM-FG-LDR-DH-TASK",
  "executionRoleArn": "arn:aws:iam::806292820880:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "1024"
}

