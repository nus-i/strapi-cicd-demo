{
    "containerDefinitions": [
        {
            "name": "strapi-app", 
            "image": "docker.io/dockerhub adamsia/test-strapi:latest", 
            "portMappings": [
                {
                    "containerPort": 80, 
                    "hostPort": 80, 
                    "protocol": "tcp"
                }
            ], 
            "essential": true, 
            "entryPoint": [ "sh","-c" ], 
            "command": [
                "/bin/sh -c docker run --expose 80 adamsia/test-strapi:latest"
            ]
        }
    ], 
    "family": "FAM-FG-LDR-DH-TASK",
    "executionRoleArn": "arn:aws:iam::806292820880:role/ecsTaskExecutionRole", 
    "networkMode": "awsvpc",
    "volumes": [],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512"
}
