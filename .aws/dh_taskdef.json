{
	"containerDefinitions": [
	{
		"name": "strapi-app",
		"image": "https://registry.hub.docker.com/v1/adamsia/test-strapi/latest",
		"cpu": 1,
		"portMappings": [
			{
				"containerPort": 1337,
				"hostPort": 1337,
				"protocol": "tcp"
			}
		],
		"essential": true,
		"entryPoint": ["sh","-c"],
		"command": [
			"docker run adamsia/test-strapi:latest"
		],
		"environment": [],
		"mountPoints": [],
		"volumesFrom": [],
		"logConfiguration": {
			"logDriver": "awslogs",
			"options": {
				"awslogs-group": "/ecs/FG-LDR-DH-TASK",
				"awslogs-region": "us-east-1",
				"awslogs-create-group": "true",
				"awslogs-stream-prefix": "ecs"
			}
		}
	  }
	],
	"family": "FG-LDR-DH-TASK",
	"taskDefinitionArn": "arn:aws:ecs:us-east-1:806292820880:task-definition/FG-LDR-DH-TASK",
        "executionRoleArn": "arn:aws:iam::806292820880:role/ecsTaskExecutionRole", 
	"networkMode": "awsvpc",
	"volumes": [],
	"requiresCompatibilities": ["FARGATE"],
	"cpu": "256",
	"memory": "512"
}
