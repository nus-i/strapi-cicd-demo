{
	"containerDefinitions": [
	{
		"name": "strapi-app",
		"image": "adamsia/test-strapi:latest",
		"cpu": 1,
		"portMappings": [
			{
				"containerPort": 80,
				"hostPort": 80,
				"protocol": "tcp"
			}
		],
		"essential": true,
		"entryPoint": ["sh","-c"],
		"command": [
			"docker pull adamsia/test-strapi:my-strapi-image2"
		],
		"environment": [],
		"mountPoints": [],
		"volumesFrom": [],
		"logConfiguration": {
			"logDriver": "awslogs",
			"options": {
				"awslogs-group": "firelens-container",
				"awslogs-region": "us-east-1",
				"awslogs-create-group": "true",
				"awslogs-stream-prefix": "firelens"
			}
		}
	  }
	],
	"family": "fg-ldr-demo-task",
        "executionRoleArn": "arn:aws:iam::806292820880:role/ecsTaskExecutionRole", 
	"networkMode": "awsvpc",
	"volumes": [],
	"requiresCompatibilities": ["FARGATE"],
	"cpu": "256",
	"memory": "512"
}
